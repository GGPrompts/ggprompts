{
  "name": "@ggprompts/themes",
  "version": "0.1.0",
  "description": "Unified theme system for GGPrompts - 9 color themes x 2 modes (dark/light)",
  "type": "module",
  "main": "./src/index.css",
  "exports": {
    ".": "./src/index.css",
    "./themes/*": "./src/themes/*",
    "./modes": "./src/modes.css",
    "./backgrounds": "./src/backgrounds.css",
    "./utilities": "./src/utilities.css"
  },
  "files": [
    "src"
  ],
  "keywords": [
    "themes",
    "css",
    "tailwind",
    "dark-mode",
    "light-mode",
    "glassmorphism"
  ],
  "author": "GGPrompts",
  "license": "MIT",
  "peerDependencies": {
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "tailwindcss": "^4.0.0"
  }
}
